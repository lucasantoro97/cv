@article{11583_2987997,
 abstract = {Questa ricerca delinea le fasi iniziali di una metodologia per la caratterizzazione fisica e meccanica degli elementi strutturali in legno di castagno in diverse condizioni di conservazione. Nell'ambito della tutela del patrimonio architettonico in legno, è cruciale ottenere stime delle proprietà fisiche (densità, trasmissività, livello di umidità) e meccaniche (modulo elastico, resistenza a compressione e flessione) degli elementi attraverso le correlazioni tra prove non distruttive (PnD), per guidare gli interventi di conservazione.},
 author = {Sesana, Raffaella and Raffaella, R and Santoro, L and Borghese, V. and Santini, S},
 journal = {INGENIO-WEB.IT   https://www.ingenio-web.it/},
 keywords = {legno; riciclo; energia},
 pages = {1--14},
 publisher = {IMREADY Srl  https://www.ingenio-web.it/},
 title = {Correlazioni Preliminari tra Proprietà Termiche e Densità nelle Strutture in Legno con Prove Non Distruttive},
 url = {https://www.ingenio-web.it/articoli/correlazioni-preliminari-tra-proprieta-termiche-e-densita-nelle-strutture-in-legno-con-prove-non-distruttive/},
 year = {2024}
}
